@{
    ViewBag.Title = "Company Register Menu";
    Layout = "_Layout";
}

<div class="container-fluid m-4">
    <h3>Company Register</h3>
    <p>List Company waiting for approval</p>
    <div class="card-box table-responsive">
        <table id="datatable-company-register" class="table table-striped table-bordered" style="width:100%">
            <thead>
            <tr>
                <th width="1%">No</th>
                <th>Name</th>
                <th>Email</th>
                <th>Telp</th>
                <th>Image</th>
                <th>Status</th>
                <th>Created Date</th>
                <th>Update Date</th>
                <th width="10%">Action</th>
            </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>
